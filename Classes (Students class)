class Student:
    name = ''
    dob = ''
    gender = ''
    rollnum = ''
    major = ''

student1 = Student()
student1.name = 'Nasser'
student1.dob = '04/10/2003'
student1.rollnum = 'Au2025'
print(student1.name, student1.dob, student1.rollnum,)

student2 = Student()
student2.name = 'Joe'
student2.dob = '02/10/1957'
student2.major = 'Political Science'

print(student2.name, student2.dob, student2.major)

print(type(student1))
