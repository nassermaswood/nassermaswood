a = int(input(': '))
b = int(input(': '))
c = int(input(': '))

d = int(input(': '))
e = int(input(': '))
f = int(input(': '))


for x in range(-10,11):
    for y in range(-10,11):
        if (a * x) + (b * y) == c and (d * x) + (e * y) == f and x != 0 and y != 0:
            print('x =', x, 'y =',y)
else:
    print('There is no solution')


      






